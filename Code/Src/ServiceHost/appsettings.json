{
  "basic": {
    "serviceName": "NEO"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "esDb": {
    "DbConnectionString": "esdb://admin:changeit@localhost:2113?tls=false",
    "SubscriptionId": "Neo-Test-Sub"
  },
  "swagger": {
    "enabled": true,
    "name": "Neo",
    "title": "Neo",
    "version": "v1",
    "routePrefix": "api/swagger",
    "basePath": "/api"
  },
  "massTransit": {
    "host": "rabbitmq://localhost",
    "username": "guest",
    "password": "guest",
    "prefetchCount": 8,
    "redisRepository": "localhost",
    "streamEventTypeStateMachineAddress": "queue:stream-event-type-machine-state",
    "streamContextStateMachineAddress": "queue:stream-context-machine-state",
    "lifeStreamStateMachineAddress": "queue:life-stream-machine-state"
  },
  "mongoDB": {
    "connectionString": "mongodb://localhost:27017",
    "databaseName": "neo"
  },
  "redis": {
    "ConnectionString": "localhost:6379, abortConnect=false"
  },
  "AllowedHosts": "*"
}
